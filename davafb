[32m+[m[32mCREATE DATABASE Bash_db;[m
[32m+[m
[32m+[m[32mCREATE TABLE users ([m
[32m+[m	[32muser_id INT AUTO_INCREMENT PRIMARY KEY,[m
[32m+[m[32m    username VARCHAR(20) UNIQUE NOT NULL,[m
[32m+[m[32m    password VARCHAR(64) NOT NULL,[m
[32m+[m[32m    name VARCHAR(100) NOT NULL,[m
[32m+[m[32m    address VARCHAR(200) NOT NULL,[m
[32m+[m[32m    aadhaar VARCHAR(12) NOT NULL,[m
[32m+[m[32m    mobile VARCHAR(10) NOT NULL,[m
[32m+[m[32m    account_number VARCHAR(10) UNIQUE NOT NULL,[m
[32m+[m[32m    balance DECIMAL(10, 2) DEFAULT 100.00[m
[32m+[m[32m);[m
[32m+[m[41m [m
[32m+[m[32mCREATE TABLE cards ([m
[32m+[m	[32mcard_id INT AUTO_INCREMENT PRIMARY KEY,[m
[32m+[m[32m    user_id INT NOT NULL,[m
[32m+[m[32m    card_number VARCHAR(16) UNIQUE NOT NULL,[m
[32m+[m[32m    card_type ENUM('Debit','Credit') NOT NULL,[m
[32m+[m[32m    pin VARCHAR(4) NOT NULL,[m
[32m+[m[32m    cvv VARCHAR(3) NOT NULL,[m
[32m+[m[32m    FOREIGN KEY(user_id) REFERENCES users(user_id)[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mCREATE TABLE transactions ([m
[32m+[m	[32mtransaction_id INT AUTO_INCREMENT PRIMARY KEY,[m
[32m+[m[32m    user_id INT NOT NULL,[m
[32m+[m[32m    amount DECIMAL(10, 2) NOT NULL,[m
[32m+[m[32m    beneficiary_account VARCHAR(10) NOT NULL,[m
[32m+[m[32m    transaction_date DATETIME NOT NULL,[m
[32m+[m[32m    FOREIGN KEY (user_id) REFERENCES users(user_id)[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32mCREATE TABLE beneficiaries ([m
[32m+[m	[32mid INT AUTO_INCREMENT PRIMARY KEY,[m
[32m+[m[32m    user_id INT NOT NULL,[m
[32m+[m[32m    name VARCHAR(100) NOT NULL,[m
[32m+[m[32m    account_number VARCHAR(10) NOT NULL,[m
[32m+[m[32m    FOREIGN KEY (user_id) REFERENCES users(user_id)[m
[32m+[m[32m);[m
