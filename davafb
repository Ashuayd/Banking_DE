[32m+[m[32mVALUES (%s, %s, %s, %s, %s, %s, %s, %s)[m
[32m+[m[32m"""[m
[32m+[m[32mparams = ([m
[32m+[m[32m    "alice123",        # username: â‰¤ 20 chars, unique[m
[32m+[m[32m    "securepass123",   # password: â‰¤ 64 chars[m
[32m+[m[32m    "Alice Smith",     # name: â‰¤ 100 chars[m
[32m+[m[32m    "123 Main St",     # address: â‰¤ 200 chars[m
